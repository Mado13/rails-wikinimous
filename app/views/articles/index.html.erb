<nav>
  <%= link_to "Wikinimous", articles_path %>
  <%= link_to "New Article", articles_new_path %>
</nav>

<div class="container">
  <ul>
    <% @articles.each do |article| %>
      <li>
        <h3><%= link_to "#{article.title}", article_path(article) %></h3>
        <p><%= article.content %></p>
      </li>
    <% end %>
  </ul>
</div>
